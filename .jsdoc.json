{
    "source": {
        "includePattern": ".+\\.(js(doc|x)|ts)?$",
        "include": [
            "src"
        ],
        "exclude": [
            "node_modules",
            "dist"
        ]
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "node_modules/minami"
    }
}